<% include header %>
    <title>Sign Up</title>
</head>
<body>
    
</body>
</html>
<h1>Sign Up</h1>
<form action="/info" method="POST">
    <table>
        <tbody>
                <tr>
                    <th>Reg. No.</th>
                    <td><input type="text" name="candReg"></td>
                </tr>
                <tr>
                    <th>Name</th>
                    <td><input type="text" name="candName"></td>
                </tr>
                <tr>
                    <th>Password</th>
                    <td><input type="password" name="candPass"></td>
                </tr>
                <tr>
                    <th>E-Mail ID</th>
                    <td><input type="email" name="candEmail"></td>
                </tr>
                <tr>
                    <th>Mobile</th>
                    <td><input type="number" name="candMob"></td>
                </tr>
                <tr>
                    <th>Branch</th>
                    <td><input type="text" name="candBranch"></td>
                </tr>
                <tr>
                    <th>Gender</th>
                    <td>
                        <select name="candGender">
                            <option value="not selected">--Choose--</option>
                            <option value="male">Male</option>
                            <option value="female">Female</option>
                        </select>
                    </td>
                </tr>
                <tr>
                    <th>D.O.B</th>
                    <td><input type="date" name="candDOB"></td>
                </tr>
        </tbody>
    </table>
    <button type="submit">Submit</button>
    <button type="reset">Reset All</button>
</form>
<% include footer %>